---
#
# Ansible managed
#
exclude_paths:
  - meta/preferences.yml
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/collections.yml
  - tests
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]
  - 'yaml'
  - 'role-name'

enable_list:
  - name[prefix]
